{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "target": "es2015",
    "downlevelIteration": true,
    "outDir": "dist",
    "rootDir": "src",
    "baseUrl": "src",
    "noEmit": false,
    "declaration": true,
    "moduleResolution": "NodeNext",
    "module": "NodeNext"
  },
  "exclude": ["vite.config.ts", "dist", "node_modules"],
  "include": ["src/**/*.ts"]
}
