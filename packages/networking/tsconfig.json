{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "dist",
    "rootDir": "src",
    "baseUrl": "src",
    "moduleResolution": "NodeNext",
    "noEmit": false,
    "declaration": true,
    "module": "NodeNext",
    "types": ["vite/client", "node"],
    "paths": {
      "@/types": ["types/index"],
      "@/shared": ["shared/index"],
      "@/telnet": ["telnet/index"],
      "@/constants": ["constants/index"],
      "@/utils": ["utils/index"],
      "@/types/*": ["types/*"],
      "@/shared/*": ["shared/*"],
      "@/telnet/*": ["telnet/*"],
      "@/constants/*": ["constants/*"],
      "@/utils/*": ["utils/*"]
    }
  },
  "include": ["src/telnet.ts"]
}
